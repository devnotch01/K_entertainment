{
  "name": "k_project",
  "version": "1.0.0",
  "description": "landing page for k_entertainment",
  "engines": {
    "node": "12.18.3"
  },
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss resources/css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss resources/css/style.comp.css",
    "concat:css": "concat -o resources/css/style.concat.css resources/css/icon-font.css resources/css/style.comp.css",
    "prefix:css": "postcss resources/css/style.concat.css --use autoprefixer -o resources/css/style.prefix.css",
    "compress:css": "node-sass resources/css/style.prefix.css resources/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/habib92i/K_entertainment.git"
  },
  "author": "habib",
  "license": "ISC",
  "browserslist": "last 10 versions",
  "bugs": {
    "url": "https://github.com/habib92i/K_entertainment/issues"
  },
  "homepage": "https://github.com/habib92i/K_entertainment#readme",
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "concat": "^1.0.3",
    "node-sass": "^4.14.1",
    "postcss": "^8.0.8",
    "postcss-cli": "^8.0.0"
  },
  "dependencies": {
    "express": "^4.17.1",
    "foobar": "1.1.0",
    "jquery": "^3.5.1"
  }
}
